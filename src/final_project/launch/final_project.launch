<launch>
    <!-- Set all nodes to use sim time -->
    <param name="use_sim_time" value="true" />
    <!-- Launch World -->
    <node pkg="stage_ros" type="stageros" name="simulator" args="$(find lab3)/worlds/manyDotsNoisyOdom.world">
    </node>
    <!-- Include nav_bundle launch file -->
    <include file="$(find nav_bundle)/launch/nav_bundle.launch"/>
    <!-- Launch frontier exploration -->
    <include file="$(find frontier_exploration)/launch/global_map.launch"/>
    <!-- Visualize with rviz -->
    <node pkg="rviz" type="rviz" name="rviz" args="-d $(find final_project)/rvizsetup.rviz"/>
</launch>