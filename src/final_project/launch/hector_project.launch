<launch>
    <!-- Set all nodes to use sim time -->
    <param name="use_sim_time" value="true" />
    <!-- Launch World -->
    <node pkg="stage_ros" type="stageros" name="simulator" args="$(find final_project)/testWorlds/mazeWorld.world">
    </node>
    <!-- Include nav_bundle launch file -->
    <include file="$(find nav_bundle)/launch/nav_bundle.launch"/>
    <!-- Launch frontier exploration -->
    <node pkg="hector_exploration_controller" type="simple_exploration_controller" name="hector_exploration_controller" output="screen"/>
    <include file="$(find hector_exploration_node)/launch/exploration_planner.launch"/>
    <!-- Visualize with rviz -->
    <node pkg="hector_path_follower" type="hector_path_follower_node" name="hector_path_follower" output="screen"/>
    <!-- <node pkg="final_project" type="frontier_to_nav.py" name="frontier_to_nav" output="screen"/> -->
    <node pkg="rviz" type="rviz" name="rviz" args="-d $(find final_project)/rvizsetup.rviz"/>
</launch>